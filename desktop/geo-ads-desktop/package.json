{
  "name": "geo-ads-desktop",
  "version": "0.1.0",
  "main": "main.js",
  "private": true,
  "scripts": {
    "backend": "cd ../backend && uvicorn app.main:app --reload",
    "frontend": "cd ../frontend/geo-ads-frontend && npm start",
